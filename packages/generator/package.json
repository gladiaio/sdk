{
  "name": "@gladiaio/generator",
  "description": "Code generator for Gladia SDK from OpenAPI schema",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "generate": "node --experimental-strip-types src/cli.ts && nx run-many -t format:write lint:fix"
  },
  "dependencies": {
    "case-anything": "^3.1.2",
    "commander": "^14.0.1",
    "openapi3-ts": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.18.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "prettier": "^3.6.2"
  },
  "engines": {
    "node": ">=22"
  }
}
